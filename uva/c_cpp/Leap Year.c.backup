#include <stdio.h>
int main()
{
    int a,b,c;
    while(scanf("%d",&a)==1)
    {
        if(a%4==0||a%400==0)
        {
            printf("This is leap year.\n");
            if(a%15==0)
            {
                printf("This is huluculu festival year.\n");
            }
            if(a%55==0)
            {
                printf("This is buluculu festival year.\n");
            }
        }
        else
        {
            if(a%15==0)
            {
                printf("This is huluculu festival year.\n");
            }
            else { printf("This is an ordinary year.\n");}

        }
        printf("\n");
    }
    return 0;
}
